{
    "domains": [
        {
            "name": "example",  // 任务名称
            "sub": ["https://example.com/subscribe"],  // 订阅链接
            "domain": "example.com",  // 域名
            "enable": true,  // 是否启用
            "rename": "",  // 重命名规则
            "include": "",  // 包含规则
            "exclude": "",  // 排除规则
            "push_to": ["xxx"],  // 推送到的组
            "ignorede": true,  // 是否忽略默认排除规则
            "liveness": true,  // 是否检查节点活性
            "tag": "",  // 自定义标签
            "rate": 2.5,  // 节点倍率
            "count": 2,  // 需要注册的账号数量
            "coupon": "",  // 优惠码
            "secure": false,  // 是否拒绝跳过证书验证
            "renew": {
                "account": [
                    {
                        "email": "",  // 注册邮箱
                        "passwd": "",  // 注册密码
                        "ticket": {
                            "enable": true,  // 是否启用工单
                            "autoreset": false,  // 是否自动重置
                            "subject": "",  // 工单主题
                            "message": "",  // 工单内容
                            "level": 1  // 工单优先级
                        }
                    }
                ],
                "plan_id": 3,  // 套餐ID
                "package": "",  // 套餐名称
                "method": 1,  // 支付方式
                "coupon_code": "",  // 优惠码
                "chatgpt": {
                    "enable": true,  // 是否启用 ChatGPT 检查
                    "regex": "",  // 正则表达式
                    "operate": "IN"  // 操作类型
                }
            }
        }
    ],
    "crawl": {
        "enable": true,  // 是否启用爬虫
        "exclude": "",  // 全局排除规则
        "threshold": 5,  // 爬取阈值
        "singlelink": true,  // 是否单链接爬取
        "persist": {
            "subs": "crawledsubs",  // 订阅持久化配置
            "proxies": "crawledproxies"  // 代理持久化配置
        },
        "config": {
            "rename": "",  // 重命名规则
            "include": "",  // 包含规则
            "exclude": "",  // 排除规则
            "xxxxxxx": ""  // 其他配置
        },
        "telegram": {
            "enable": true,  // 是否启用 Telegram 爬虫
            "pages": 5,  // 爬取页数
            "exclude": "",  // 排除规则
            "users": {
                "channel": {
                    "include": "",  // 包含规则
                    "exclude": "",  // 排除规则
                    "config": {
                        "rename": "",  // 重命名规则
                        "xxxxxx": ""  // 其他配置
                    },
                    "push_to": []  // 推送到的组
                }
            }
        },
        "google": {
            "enable": false,  // 是否启用 Google 爬虫
            "exclude": "",  // 排除规则
            "notinurl": [],  // 不在 URL 中的关键词
            "push_to": []  // 推送到的组
        },
        "github": {
            "enable": true,  // 是否启用 GitHub 爬虫
            "pages": 2,  // 爬取页数
            "push_to": [],  // 推送到的组
            "exclude": "",  // 排除规则
            "spams": []  // 垃圾关键词
        },
        "twitter": {
            "enable": true,  // 是否启用 Twitter 爬虫
            "users": {
                "username": {
                    "enable": true,  // 是否启用该用户爬虫
                    "num": 30,  // 爬取数量
                    "include": "",  // 包含规则
                    "exclude": "",  // 排除规则
                    "config": {
                        "rename": "",  // 重命名规则
                        "xxxxxx": ""  // 其他配置
                    },
                    "push_to": []  // 推送到的组
                }
            }
        },
        "repositories": [
            {
                "enable": false,  // 是否启用仓库爬虫
                "username": "",  // GitHub 用户名
                "repo_name": "",  // 仓库名称
                "commits": 3,  // 爬取提交数量
                "exclude": "",  // 排除规则
                "push_to": []  // 推送到的组
            }
        ],
        "pages": [
            {
                "enable": true,  // 是否启用页面爬虫
                "url": "",  // 页面 URL
                "include": "",  // 包含规则
                "exclude": "",  // 排除规则
                "config": {
                    "rename": ""  // 重命名规则
                },
                "push_to": []  // 推送到的组
            }
        ],
        "scripts": [
            {
                "enable": false,  // 是否启用脚本爬虫
                "script": "file#function",  // 脚本路径
                "params": {
                    "persist": {
                        "fileid": ""  // 文件 ID
                    },
                    "any": "xxx",  // 其他参数
                    "config": {
                        "enable": true,  // 是否启用
                        "liveness": true,  // 是否检查活性
                        "exclude": "",  // 排除规则
                        "rename": "",  // 重命名规则
                        "push_to": []  // 推送到的组
                    }
                }
            }
        ]
    },
    "groups": {
        "xxx": {
            "emoji": true,  // 是否启用 Emoji
            "list": true,  // 是否启用列表
            "targets": {
                "clash": "xxx-clash",  // Clash 目标
                "singbox": "xxx-singbox",  // SingBox 目标
                "v2ray": "xxx-v2ray"  // V2Ray 目标
            },
            "regularize": {
                "enable": false,  // 是否启用正则化
                "locate": true,  // 是否启用定位
                "bits": 2  // 位数
            }
        }
    },
    "storage": {
        "engine": "local",  // 存储引擎（local 或 file）
        "items": {  
            "xxx-clash": {
                "type": "file",  // 存储类型
                "path": "/path/to/clash.txt"  // 文件路径
            },
            "xxx-singbox": {
                "type": "file",  // 存储类型
                "path": "/path/to/singbox.txt"  // 文件路径
            },
            "xxx-v2ray": {
                "type": "file",  // 存储类型
                "path": "/path/to/v2ray.txt"  // 文件路径
            },
            "crawledsubs": {
                "type": "file",  // 存储类型
                "path": "/path/to/crawledsubs.txt"  // 文件路径
            },
            "crawledproxies": {
                "type": "file",  // 存储类型
                "path": "/path/to/crawledproxies.txt"  // 文件路径
            }
        }
    }
}
